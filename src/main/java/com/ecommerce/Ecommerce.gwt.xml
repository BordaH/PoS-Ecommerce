<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN"
        "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to="Ecommerce">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="org.gwtbootstrap3.GwtBootstrap3"/>
    <inherits name="org.gwtbootstrap3.extras.notify.Notify"/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.ecommerce.client.Ecommerce'/>
    <source path="client"/>
    <source path='dominio'/>
    <!-- Specify the app servlets.                   -->
    <servlet path='/EcommerceService' class='com.ecommerce.server.EcommerceServiceImpl'/>

</module>